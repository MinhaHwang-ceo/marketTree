main .layout-order-wrap .layout-order_step .step_num{ display: flex; width: 50%; text-align: right; justify-content: flex-end; align-items: center; font-size: 1.15em; color: #898989; }
main .layout-order-wrap .layout-order_step .step_num ul{}
