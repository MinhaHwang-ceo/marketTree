/******************************************************************************
* 장바구니 공통
******************************************************************************/
main .layout-order-wrap {width:100%;margin: 0px auto;text-align:center;/* padding-top: 50px; */}
main .layout-order-wrap .layout-order_step{padding: 70px 0 35px;display: flex;flex-direction: row;justify-content: space-between;border-bottom: 2px solid #ababab;}
main .layout-order-wrap .layout-order_step h4{color: #323232;font-size: 40px;font-weight: 500;display: flex;flex-direction: row;justify-content: flex-start;font-weight: 600;}
main .layout-order-wrap .layout-order_step h4 .step_text{font-size: 0.9rem;font-weight: 400;text-align:left;position:relative;margin-left: 20px;padding-left: 15px;align-items: center;flex: auto;line-height: 1.4em;color: #747474;margin-top: 2px;}
main .layout-order-wrap .layout-order_step h4 .step_text:before{display: block;content:'';width:1px;height: 37px;margin-right: 15px;background: #ddd;position:absolute;left: 0;vertical-align: middle;}
