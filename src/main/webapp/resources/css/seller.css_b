/******************셀러페이지 CSS**************************/

._buybg, ._sellbg{/* background: #f6f9fc; *//* width:100% !important; *//* width: calc(100% - 60px) !important; *//* margin-left: 60px; */}

.bs_inwrap{width: 100%;margin: 0 auto;/* height: 100%; */position: relative;}
.bs_inwrap:after{display:block; clear:both; content:'';}


/**********************buy, seller lnb**********************/
/*lnb*/
.buy_lnb,
.sell_lnb{/* float:left; */width: 250px;background: #555558;height: 100%;padding: 15px 0px;box-sizing: border-box;position: absolute;}



/*buy,seller 선택*/
.division_area{/* margin: 15px 20px 20px; */padding: 15px 20px 20px;background: #dcdcdc;border-radius: 0;}
.division_area ul{*zoom:1;width: 100%;background: #000;border-radius: 150px;}
.division_area ul:after{ display:block; clear:both; content:'';}
.division_area ul > li{float:left; width:calc(100%/2);}
.division_area ul > li > a{display:block;width:100%;color: #000;text-align: center;padding: 9px 0;box-sizing: border-box;font-size: 14px;}
.division_area ul > li.on > a.buyer{background:#000;color:#fff}
.division_area ul > li.on > a.seller{background: #1b1b1b;
    color: #fff;
    border-radius: 10px;
    font-size: 15px;}

/*seller menu*/

.buyer_lnb, .seller_lnb {
    float: left;
    width: 200px;
    background: #ffffff;
    /* height: 100%; */
    /* padding: 15px 0px; */
    box-sizing: border-box;
    position: absolute;
    border: 1px solid #d4d4d4;
    border-radius: 5px;
    border-bottom: 0px;
}



/*1depth*/
.seller_menu{margin: 0;}
.seller_menu > li{padding: 0 10px;border-bottom: 1px solid #dedede;}
.seller_menu > li:hover{
    background: #ffffff;
    /* border-bottom: 0px solid #454546;}
.seller_menu > li:hover > a{/* padding: 0; */
    /* border-bottom: 1px solid #4f4f50; */
    /* padding: 10px; */
    /* box-sizing: border-box; */
    }
.seller_menu > li > a{
    color: #555558;
    padding: 10px;
    font-size: 14px;
    position: relative;
    width: 100%;
    margin: 0 auto;
    display: block;
    box-sizing: border-box;
    padding: 15px 10px;
    /* border-bottom: 0px solid #ffffff;}
.seller_menu > li > a > i{margin-right:7px; font-size:17px; vertical-align:baseline; }
.seller_menu > li > a:after {content:'';display:block;position:absolute;right: 5px;top:50%;width:7px;height:7px;margin: -1px 0 0;border-left: 1px solid #a2a2a2;border-top: 1px solid #a2a2a2;border-right: 0px solid #fff;border-bottom: 0px solid #fff;-ms-transform:rotate(45deg);-webkit-transform:rotate(45deg);transform:rotate(45deg);}
.seller_menu > li.on:hover{}
.seller_menu > li.on{padding: 10px 0px;background: #555558;}
.seller_menu > li.on > a{color: #00fb92;padding: 10px;font-size: 16px;position: relative;width: 85%;margin: 0 auto;display: block;box-sizing: border-box;/* padding: 10px; */
    /* font-weight: 600; */
    }
.seller_menu > li.on > a:after {content:'';display:block;position:absolute;right: 5px;top:50%;width:7px;height:7px;margin:-7px 0 0;border-left: 0px solid #fff;border-top: 0px solid #fff;border-right: 1px solid #fff;border-bottom: 1px solid #fff;-ms-transform:rotate(45deg);-webkit-transform:rotate(45deg);transform:rotate(45deg);}

/*2depth*/
.seller_menu > li > ul.depth2{ padding: 0px 10px 10px;
    /* background: #dcdcdc; */
    margin-top: 15px;
    /* border-radius: 5px; */
    width: 100%;
    box-sizing: border-box;
    width: calc(100% - 15px);
    margin: 0 auto;
    margin-left: 15px;
    /* border-top: 1px solid #d2d2d2; */
    /* border-bottom: 1px solid #5f5f5f; */
    box-sizing: border-box;}
.seller_menu > li > ul.depth2 > li{padding: 5px;}
.seller_menu > li > ul.depth2 > li > a{padding: 0px 0 0px;font-size: 15px;color: #f9f9f9;font-size: 14px;}
.seller_menu > li > ul.depth2 > li > a:before{display:inline-block;content:'-';margin-right: 5px;}


/**********************seller body**********************/
/*body 영역*/
.bs_content{/* float:left; *//* width: calc(100% - 320px); *//* margin-left: 290px; */padding-top: 30px;}

/*depth3 - TAB*/
.bs_content .depth3_menu{}
.bs_content .depth3_menu ul{}
.bs_content .depth3_menu ul:after{display:block; clear:both; content:'';}
.bs_content .depth3_menu ul > li{float:left;position: relative;/* border-bottom: 1px solid #000; */width: calc(100%/3);box-sizing: border-box;border: 1px solid #ddd;border-bottom: 1px solid #000;}
.bs_content .depth3_menu ul > li:after{display:block;/* content:''; */width:1px;height: 18px;background: #bbbaba;position:absolute;right:0;top:10px;}
.bs_content .depth3_menu ul > li:last-child:after,
.bs_content .depth3_menu ul > li.on:after{display:none; }
.bs_content .depth3_menu ul > li > a{display:block;padding: 15px 20px 14px;font-size: 1.3rem;color: #0a0a0a;text-align: center;background: #efefef;}
.bs_content .depth3_menu ul > li.hover > a{font-weight:600; color;#000}
.bs_content .depth3_menu ul > li.on{border: 1px solid #000;border-bottom: 0px solid #f9f9f9;}
.bs_content .depth3_menu ul > li.on > a{display:block;padding: 15px 20px 15px;/* font-size: 1.0rem; */color: #000000;font-weight: 600;/* font-weight: 400; */background: #fbfbfb;}

.bs_content h3:before,
.bs_content h4:before,
.bs_content h5:before,
.bs_content h6:before{display:inline-block; content:''; vertical-align: middle;}

.bs_content h3{border-bottom: 0px !important;color: #505050 !important;padding-top: 25px !important;margin-bottom: 20px !important;font-weight: 600;font-size: 25px !important;}

.bs_content h4{font-size: 1.1rem !important;margin-bottom: 15px;color: #242525 !important;padding-top: 20px;padding-left: 5px;}
/*.bs_content h4:before {width: 5px;height: 5px;background: #fff;vertical-align: middle;margin-right: 5px;margin-left: 0px;margin-top: -5px;}*/

.bs_content h5{font-size: 18px !important;margin-bottom: 12px;color: #292929 !important;display:inline-block;position: relative;/* width: calc(100% - 10px); */vertical-align: middle;width: 100%;font-weight: 500;}
.bs_content h5 + textarea{width:100%}
.bs_content h5>span{
    font-size: 13px;
    line-height: 19px;
    color: #f04b45;
    letter-spacing: -.05em;
    margin-left: 10px;
    }

.bs_content h5:before {width: 5px;height: 5px;background: #148ca7;margin-right: 5px;margin-top: -5px;display: none;}
.bs_content h5 .inputbox{
    display: inline-block;
    /* width: calc(100% - 200px); */
    text-align: right;
    position: absolute;
    right: -3px;
    top: -5px;
    padding-bottom: 0;
    padding: 0px;
}
.bs_content h5 .field{display:inline-block;vertical-align: middle;margin-left: 15px;position: relative;margin-left: 35px;margin-right: 10px;}
.bs_content h5 .field + .field{margin-left:10px}
.bs_content h5 + .part04{border-top:1px solid #ababab; padding-top:5px; margin-top:5px}
.bs_content h5 +.field{display:inline-block;vertical-align: middle;margin-left: 15px;margin-top: -7px;}

.bs_content h5 > em{position:absolute;right: 10px;font-weight: 400;font-size: 14px;top: 0;}
.bs_content h5 + .swich_area{margin-top: 10px;}

.bs_content h6{}



/**********************seller body**********************/
/*body 영역*/
.seller_body{float:left;width: calc(100% - 250px);margin-left: 250px;}

/*depth3 - TAB*/
.seller_body .depth3_menu{}
.seller_body .depth3_menu ul{}
.seller_body .depth3_menu ul:after{display:block; clear:both; content:'';}
.seller_body .depth3_menu ul > li{float:left;position: relative;/* border-bottom: 1px solid #000; */width: calc(100%/3);box-sizing: border-box;border: 1px solid #ddd;border-bottom: 1px solid #777373;}
.seller_body .depth3_menu ul > li:after{display:block;/* content:''; */width:1px;height: 18px;background: #bbbaba;position:absolute;right:0;top:10px;}
.seller_body .depth3_menu ul > li:last-child:after,
.seller_body .depth3_menu ul > li.on:after{display:none; }
.seller_body .depth3_menu ul > li > a{display:block;padding: 15px 20px 14px;font-size: 1.3rem;color: #0a0a0a;text-align: center;/* background: #e6e4e4; */}
.seller_body .depth3_menu ul > li.hover > a{font-weight:600; color;#000}
.seller_body .depth3_menu ul > li.on{border: 1px solid #777373;border-bottom: 0px solid #f9f9f9;}
.seller_body .depth3_menu ul > li.on > a{/* display:block; *//* padding: 15px 20px 15px; *//* font-size: 1.0rem; *//* color: #000000; *//* font-weight: 600; *//* font-weight: 400; *//* background: #ffffff; */}

.seller_body h3:before,
.seller_body h4:before,
.seller_body h5:before,
.seller_body h6:before{display:inline-block; content:''; vertical-align: middle;}

.seller_body h3{/* border-bottom: 0px !important; *//* color: #505050 !important; *//* padding-top: 25px !important; *//* margin-bottom: 20px !important; *//* font-weight: 600; *//* font-size: 30px !important; */}

.seller_body h4{font-size: 1.3rem !important;margin-bottom: 15px;color: #242525 !important;padding-top: 20px;padding-left: 5px;}
/*.seller_body h4:before {width: 5px;height: 5px;background: #fff;vertical-align: middle;margin-right: 5px;margin-left: 0px;margin-top: -5px;}*/

.seller_body h5{font-size: 18px !important;margin-bottom: 5px;color: #046980 !important;display:inline-block;position: relative;/* width: calc(100% - 10px); */vertical-align: middle;width: 100%;}
.seller_body h5:before {width: 5px;height: 5px;background: #148ca7;margin-right: 5px;margin-top: -5px;display: none;}
.seller_body h5 .field{display:inline-block;vertical-align: middle;margin-left: 15px;position: absolute;margin-left: 35px;}
.seller_body h5 + .part04{border-top:1px solid #ababab; padding-top:5px; margin-top:5px}
.seller_body h5 +.field{display:inline-block;vertical-align: middle;margin-left: 15px;margin-top: -7px;}

.seller_body h5 > em{position:absolute;right: 10px;font-weight: 400;font-size: 14px;top: 0;}
.seller_body h5 + .swich_area{/* margin-top: 10px; */}

.seller_body h6{}



/*btn*/
.bs_btn_normal{display: inline-block;text-align: center;box-sizing: border-box;background-color: #fff;border: 1px solid #929292;padding: 8px 18px;box-sizing: border-box;height: 34px;border-radius: 5px;vertical-align: middle;font-size: 15px;background: #8e8e8e;color: #fff;}


/*타이틀*/

.box_section .tit_area{ margin: 5px 5px 5px; }
.box_section .tit_area > h5{vertical-align:middle;}
.box_section .tit_area > h5 + .field{display:inline-block;vertical-align:middle; margin-left:15px}
.box_section .tit_area > h5 + input[type="checkbox"]{margin-left:15px}


/*section영역*/
.bs_content .section{margin-bottom: 0;padding-top: 25px;}
.bs_content .section + .section{margin-top:45px}
.bs_content .section .box_area{background: #ffffff;padding: 40px 50px;border-radius: 5px;border: 1px solid #efefef;}
.bs_content .section .box_area ul[class^="part"]{width:100%;margin-bottom: 35px;border-bottom: 1px solid #e8e8e8;padding-bottom: 35px;}
.bs_content .section .box_area ul[class^="part"]:after{display:block; clear:both; content:'';}

/*part common*/

.bs_content .section .box_area .part02>li,
.bs_content .section .box_area .part03>li,
.bs_content .section .box_area .part04>li{float:left; padding:0; box-sizing:border-box; padding-right:10px;}

/*part01*/
.bs_content .section .box_area .part01{padding: 0;}
.bs_content .section .box_area .part01 + .part01{margin-top:10px}
.bs_content .section .box_area .part01 + .part03{margin-top:10px}
.bs_content .section .box_area .part01 > li{padding-bottom: 0;}
.bs_content .section .box_area .part01 > li > .inputbox{padding-left:5px}
.bs_content .section .box_area .part01 > li>span{display: inline-block;width: 200px;font-size: 15px;font-weight: 600;padding-right: 10px;box-sizing: border-box;text-align: justify;position: relative;/* padding-left: 0px; *//* vertical-align: middle; *//* float: left; */color: #000;}
.bs_content .section .box_area .part01 > li>span.r_line{display:inline-block;width: 180px;font-size: 15px;font-weight: 500;padding-left: 20px;padding-right: 10px;box-sizing: border-box;text-align: justify;position: relative;/* padding-left: 0px; */}
.bs_content .section .box_area .part01 > li>span.r_line:after{display:inline-block;width: 1px;height: 20px;background: #c7c7c7;content:'';position: absolute;margin-left: 5px;right: 10px;/* margin-right: 15px; */}
.bs_content .section .box_area .part01 > li>span + .inputbox{display:inline-block;width: calc(100% - 210px);vertical-align: middle;padding-left: 10px;box-sizing: border-box;}
.bs_content .section .box_area .part01 li span + .inputbox select{display:inline-block;width: calc((100% /2) - 15px);margin-left: 5px;}
.bs_content .section .box_area .part01 > li>._box02{display:inline-block; width: calc(100%/2);}
.bs_content .section .box_area .part01 > li>._box03{display:inline-block;width: calc(100% - 210px);padding-left: 10px;box-sizing: border-box;vertical-align: middle;}
.bs_content .section .box_area .part01 > li>._box03:after{display:block; clear:both; content:'';}
.bs_content .section .box_area .part01 > li>._box03>.inputbox{
    float: left;
    margin-right: 0;
    width: calc(100%/3 - 10px);
}
.bs_content .section .box_area .part01 > li>._box03>.inputbox + .notice_txt{padding-top: 35px;clear: both;padding-left: 0px;}
.bs_content .section .box_area .part01 li>span.rowspan2{height:85px}

/*part02*/
.bs_content .section .box_area .part02 + .part02{margin-top:25px;}
.bs_content .section .box_area .part02 li{width: calc(100%/2 - 15px);padding-left: 0;padding-right: 0;}
.bs_content .section .box_area .part02 li:first-child{margin-right:15px}
.bs_content .section .box_area .part02 li:last-child{margin-left:15px}
.bs_content .section .box_area .part02 > h5{display:block}

/*part03*/
.bs_content .section .box_area .part03{/* border-bottom:1px solid #333 */}
.bs_content .section .box_area .part03>li{width: calc((100%/3) - 15px);margin-right: 22px;}
.bs_content .section .box_area .part03 li:last-child{padding-right:0px;margin-right: 0;}
._plus .part03 +.part03{margin-top:5px}

/*part03*/
.bs_content .section .box_area .part04{}
.bs_content .section .box_area .part04 li{width:calc(100%/2);}
.bs_content .section .box_area .part04 li>span{display:inline-block;width:150px;font-size: 16px;font-weight: 500;padding-right: 10px;box-sizing: border-box;padding-left: 10px;text-align: justify;color: #000;}
.bs_content .section .box_area .part04 li>span + .inputbox{display:inline-block;width: calc(100% - 160px);vertical-align: middle;}
.bs_content .section .box_area .part04 li + li>span{padding-left:20px;}

.bs_content .section .box_area>table{width:100%;}
.bs_content .section .box_area>table tr th{font-size:15px;}
.bs_content .section .box_area>table tr td{}



.bs_content .section .box_area .box_section{border-bottom: 0px solid #ffffff;padding-bottom: 15px;padding: 10px 10px 15px;}
.bs_content .section .box_area .box_section + .box_section{margin-top: 25px;}
.bs_content .section .box_area .box_section:last-child{padding-bottom:0px; border-bottom:0px;}
.bs_content .section .box_area .box_section > ul{/* position: relative; */}
.bs_content .section .box_area .box_section > ul:after{ display:block; clear:both; content:'';}
.bs_content .section .box_area .box_section > ul > li{/* float:left; *//* width:calc(100%/2); *//* padding: 0; *//* box-sizing: border-box; *//* padding-right: 10px; */}
.bs_content .section .box_area .box_section > ul > li:last-child{padding-right:0px}


.list_box{border-top: 0px solid #d0cfcf;/* margin-top: 25px; */padding: 15px 0 0;}
.list_box .list_total{padding: 10px 5px 15px;font-weight: 600;}
.list_box .list_total>strong{font-weight:600; color:#ca4100}

.list_box .btn_area{ padding-bottom: 10px; }
.list_box .btn_area:after{display:block; clear:both; content:'';}
.list_box .btn_area._toparea{}
.list_box .btn_area._bottomarea{margin-top:15px;}
.list_box .btn_area .notice_txt{font-size:15px;line-height:20px;}
.list_box .btn_area .l_btn{ float: left; }
.list_box .btn_area .r_btn{ float: right; }

.list_box .btn_area .r_btn>label{display:inline-block; margin-right:10px;}
.list_box .btn_area .r_btn>label + input{display:inline-block;width:150px;height: 35px;padding: 0 10px;}
.list_box .btn_area .r_btn>input + label{margin-left:15px}
.list_box .btn_area .r_btn>select{width:110px;padding: 0 7px;height: 35px;vertical-align: middle;}
.list_box .btn_area .r_btn>select + button{margin-left:10px}



.list_box >table{border: 1px solid #c7c7c7;width:100%;background:#fff;}
.list_box >table tr td>input[type="checkbox"]{width: 20px;height: 20px;border: 1px solid #ececec;}
.list_box >table thead tr td{ background-color: #d6d6d6; height: 43px; border-top: 1px solid #c7c7c7; border-bottom: 1px solid #c7c7c7; text-align:center; padding:10px 0; }
.box_section .list_box >table tbody tr td{height: 43px;border-bottom: 1px solid #c7c7c7;padding:10px 0;text-align:center;}

.list_box .table_g{border: 1px solid #cecece;border-top: 0px;}
.list_box .table_g .thead{background-color: #d6d6d6;border-top: 1px solid #c7c7c7; border-bottom: 1px solid #c7c7c7;padding: 10px 0;}
.list_box .table_g .thead>div{width: calc(100%/7.1);border-right: 1px solid #a2a2a2;display:inline-block;text-align:center;padding: 5px 0;box-sizing: border-box;}
.list_box .table_g .thead>div:last-child{border-right:0px}
.list_box .table_g .p_status{}
.list_box .table_g .reason{}
.list_box .table_g .code{}
.list_box .table_g .s_status{}
.list_box .table_g .categori{}
.list_box .table_g .g_name{}
.list_box .table_g .price{}

.list_box .table_g .tbody{background-color: #fff;border-top: 0px solid #c7c7c7;border-bottom: 0px solid #c7c7c7;padding: 10px 0;text-align:center;vertical-align:middle;min-height: 400px;}
.list_box .table_g .tbody>span{display: block;/* text-align: center; *//* vertical-align: middle; */padding: 150px 0;font-size: 25px;}

.goodsinfo_area{margin-bottom:30px;}


.switch_area{display:inline-block; margin: 5px 5px 20px; width:33.333%}
.switch_area ul{*zoom:1;width: 100%;background: #e6e4e4;border-radius: 5px;overflow: hidden; display: inline-block;vertical-align: middle;margin-right: 15px;}
.switch_area ul:after{ display:block; clear:both; content:'';}
.switch_area ul > li{float:left;width:calc(100%/2);border-radius: 5px;overflow: hidden;}
.switch_area ul > li > a{display:block;width:100%;height: 42px;line-height: 20px;color: #5f5f5f;text-align: center;padding: 9px 0;box-sizing: border-box;font-size: 16px;box-shadow: inset 0 0 2px #00000021;}
.switch_area ul > li.on > a.cate_sel{background: #00fb92;color: #0e5a3a;border: 1px solid #00e485;font-weight: 500;}
.switch_area ul > li.on > a.cate_search{background: #35daff;color: #000;border-radius: 150px;font-size: 15px;}




.cate_selbox{padding: 0;border-top: 2px solid #dad8d8;box-sizing: border-box;margin: 10px 0px 0;}
.cate_selbox > ul{ width: 100%; }
.cate_selbox > ul:after{ display:block; clear:both; content:'';}
.cate_selbox > ul > li{float:left;width: calc(100%/4);padding: 0 5px 0 0;box-sizing: border-box;}
.cate_selbox > ul > li:last-child{padding:0px;}
.cate_selbox > ul li > select{width: 100%;/* height: 35px; */font-size:15px;/* padding: 7px; *//* border:1px solid #CCC; *//* padding:0px 0px; *//* outline:none; *//* box-sizing:border-box; *//* vertical-align:middle; */}


.cate_searchgroup{padding: 10px 0px;border-top: 0px solid #dad8d8;box-sizing: border-box;margin: 0 7px;}
.cate_searchgroup > input[type="text"]{width: calc(100% - 74px) !important;}
.cate_searchgroup > input[type="text"] + a.bs_search{display:inline-block;/* width:100px; */box-sizing:border-box;display: inline-block;text-align: center;box-sizing: border-box;background-color: #fff;border: 1px solid #929292;/* width: 100px; */padding: 8px 18px;box-sizing: border-box;height: 34px;border-radius: 2px;vertical-align: middle;font-size: 15px;background: #8e8e8e;color: #fff;margin-left: 5px;}

.notice_txt{font-size: 13px;margin-top: 10px;color: #8e8e8e;padding-left: 10px; line-height:20px}
.notice_txt > span{display:block;color: #737272;font-weight:500;margin-top: 5px;font-size: 13px;}
.notice_txt > em{color:red}

.box_area .box_section  > table {border:0;width: 100%;border-top: 0px solid #2c2424;border-bottom: 1px solid #CCC;border-collapse:collapse;/* margin-left:80px; */}
.box_area .box_section > table tr th > span{color: #fb2c0a;font-size:14px;vertical-align:middle;}
.box_area .box_section > table tr th,
